{
  "name": "vite-plugin-md-preview-monorepo",
  "private": true,
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "homepage": "https://github.com/vitejs/vite-plugin-md-preview/",
  "repository": "git@github.com:JasKang/vite-plugin-md-preview.git",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./component": {
      "types": "./dist/component.d.ts",
      "import": "./dist/component.js",
      "require": "./dist/component.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev:vite": "pnpm --filter test-vite dev",
    "dev:vitepress": "pnpm --filter test-vitepress dev",
    "build:docs": "vite build",
    "build": "pnpm --filter vite-plugin-md-preview build",
    "test": "vitest",
    "coverage": "vitest --coverage"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@jaskang/config": "^0.0.7",
    "@jaskang/eslint-config": "^0.0.7",
    "@types/node": "^16.18.11",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "eslint": "^8.31.0",
    "prettier": "^2.8.2",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-inspect": "^0.7.14",
    "vitest": "^0.26.3",
    "vue": "^3.2.45"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  }
}
